package teste1;

import java.util.Scanner;

public class Calculos {

	int a;
	int b;
	int soma;
	
public void add() {
        
		
		Scanner scan = new Scanner(System.in);
		System.out.println("digite o primeiro numero");
		a = scan.nextInt() ;
	
		System.out.println("digite o segundo numero");
		b = scan.nextInt() ;
		soma = a+b;

		scan.close();

		
	}
	
}
