package CalcTeste;

import java.util.Scanner;

public class Calculos {

	int a;
	int b;
	double soma;

	Scanner scan = new Scanner(System.in);

	public double summ() {

		return a + b;

	}

	public double subtract() {

		return a - b;

	}

	public double division() {

		return a / b;

	}

	public double multiplication() {

		return a * b;

	}

}
