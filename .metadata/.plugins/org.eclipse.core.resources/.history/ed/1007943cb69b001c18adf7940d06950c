package teste1;

import java.util.Scanner;

public class Calculos {

	
public int add() {
        
		
		int a;
		int b;
		int soma;
		Scanner scan = new Scanner(System.in);
		System.out.println("digite o primeiro numero");
		a = scan.nextInt() ;
	
		System.out.println("digite o segundo numero");
		b = scan.nextInt() ;
		soma = a+b;
		
		return soma;

		
	}
	
}
